/* pages/search/search.wxss */
.search_box{
     display: flex;
     align-items: center;
}
.search{
     flex: 12;
}
.search_qx{
     color: #ff9822;
     flex: 2;
     height: 125rpx;
     display: flex;
     align-items: center;
     background-color: #ffffff;
}
.search-body {
     display: flex;
     flex-wrap: wrap;
     padding-bottom: 32rpx;
     height: 100%;
   }
   
   .empty {
     display: flex;
     justify-content: center;
     align-items: center;
     width: 100%;
     height: 600rpx;
   }
   
   .empty .title {
     font-size: 14px;
     color: gray;
   }
   